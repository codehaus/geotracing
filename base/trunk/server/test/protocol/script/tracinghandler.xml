	<test id="t-trk-write-nmea-req">
		<send id="client1">
			<utopia-req>
				<t-trk-resume-req/>
			</utopia-req>
			<utopia-req>
				<t-trk-write-req>
					<pt ar="1" sr="2" nmea="GPGGA,123511.044,5213.5053,N,00449.0491,E,1,05,3.3,50.5,M,47.0,M,73.7,0000*71"/>
				</t-trk-write-req>
			</utopia-req>
			<utopia-req>
				<t-trk-write-req>
					<pt ar="2" sr="2" nmea="GPGGA,124249.017,5212.8792,N,00449.1183,E,1,05,3.4,43.8,M,47.0,M,73.7,0000*73"/>
				</t-trk-write-req>
			</utopia-req>
			<utopia-req>
				<t-trk-write-req>
					<pt ar="3" sr="2" nmea="GPRMC,131246.908,A,5211.3596,N,00445.6281,E,0.00,174.73,190605,,*0A"/>
				</t-trk-write-req>
			</utopia-req>
			<utopia-req>
				<t-trk-suspend-req/>
			</utopia-req>
		</send>

		<expect id="client1">
			<utopia-rsp>
				<t-trk-resume-rsp id="${trkid4}"/>
			</utopia-rsp>
			<utopia-rsp>
				<t-trk-write-rsp/>
			</utopia-rsp>
			<utopia-rsp>
				<t-trk-write-rsp/>
			</utopia-rsp>
			<utopia-rsp>
				<t-trk-write-rsp/>
			</utopia-rsp>
			<utopia-rsp>
				<t-trk-suspend-rsp id="${trkid4}"/>
			</utopia-rsp>
		</expect>
	</test>


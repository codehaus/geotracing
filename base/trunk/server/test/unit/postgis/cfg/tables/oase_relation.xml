<?xml version="1.0" encoding="iso-8859-1"?>

<table>

    <views>

		<insert>
			<field name="rec1" />
			<field name="rec2" />
			<field name="reltype" />
			<field name="tag" />
			<field name="rank" />
			<field name="state" />
		</insert>

		<list>
			<field name="id" />
			<field name="rec1" />
			<field name="rec2" />
			<field name="reltype" />
			<field name="tag" />
			<field name="rank" />
			<field name="state" />
		</list>

		<query>
			<field name="id" />
			<field name="rec1" />
			<field name="rec2" />
			<field name="reltype" />
			<field name="tag" />
			<field name="rank" />
			<field name="state" />
		</query>
 	</views>

	<data>

		<field name="id" type="INTEGER" required="true" key="true" />

		<field name="rec1" type="INTEGER" required="true" index="true"/>

		<field name="rec2" type="INTEGER" required="true" index="true" />

		<field name="reltype" type="INTEGER" required="true" />

		<field name="tag" type="STRING" size="32" />

		<field name="rank" type="INTEGER" required="true" default="1" />

		<field name="state" type="INTEGER" required="true" default="1" />
	</data>


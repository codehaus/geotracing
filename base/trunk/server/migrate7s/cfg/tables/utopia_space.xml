<?xml version="1.0" encoding="utf-8"?><table>    <views>		<insert>			<field name="name" />			<field name="description"/>			<field name="access"/>			<field name="password"/>             <field name="expirationdate" />            <field name="extra" />		</insert>		<list>			<field name="id" />			<field name="owner" />			<field name="name" />            <field name="access"/>            <field name="password"/>            <field name="expirationdate" /> 		</list>		<query>			<field name="id" />			<field name="owner" />			<field name="name" />			<field name="description" />            <field name="access"/>            <field name="password"/>            <field name="creationdate" />            <field name="modificationdate" />            <field name="expirationdate" />            <field name="extra" />		</query> 	</views>	<data>		<field name="id" type="INTEGER" required="true" key="true" />		<field name="owner" type="STRING" size="12" required="true" default="unknown" />		<field name="name" type="STRING" size="64" required="true" />		<field name="description" type="TEXT" required="false" />		<field name="access" type="STRING" size="64" required="true" />		<field name="password" type="STRING" size="64" required="true" />        <field name="creationdate" type="TIMESTAMP"/>        <field name="modificationdate" type="TIMESTAMP"/>        <field name="expirationdate" type="TIMESTAMP"/>		<field name="extra" type="XML" />	</data></table>
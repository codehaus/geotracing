<?xml version="1.0" encoding="utf-8"?>

<table>

    <views>

		<insert>
			<field name="table1" />
			<field name="table2" />
			<field name="assoctype" />
		</insert>

		<list>
			<field name="id" />
			<field name="table1" />
			<field name="table2"  />
 			<field name="assoctype" />
		</list>

		<query>
			<field name="id" />
			<field name="table1" />
			<field name="table2" />
			<field name="assoctype" />
		</query>
 	</views>

	<data>

		<field name="id" type="INTEGER" required="true" key="true" />

		<field name="table1" type="STRING" required="true" size="32" />

		<field name="table2" type="STRING" required="true" size="32" />

		<field name="assoctype" type="INTEGER" required="true" default="1" />

	</data>


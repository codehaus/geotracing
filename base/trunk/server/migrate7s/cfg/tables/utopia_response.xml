<?xml version="1.0" encoding="utf-8"?><table>    <views>		<insert>			<field name="description"/>            <field name="classification"/>             <field name="extra" />		</insert>		<list>			<field name="id" />			<field name="owner" />            <field name="classification"/> 		</list>		<query>			<field name="id" />			<field name="owner" />			<field name="description" />            <field name="classification"/>            <field name="creationdate" />            <field name="modificationdate" />            <field name="extra" />		</query> 	</views>	<data>		<field name="id" type="INTEGER" required="true" key="true" />		<field name="owner" type="STRING" size="12" required="true" default="unknown" />		<field name="description" type="TEXT" required="true" />        <field name="classification" type="INTEGER" required="false" />        <field name="creationdate" type="TIMESTAMP"/>        <field name="modificationdate" type="TIMESTAMP"/>		<field name="extra" type="XML" />	</data></table>
<?xml version="1.0" encoding="utf-8"?><table>    <views>		<insert>			<field name="loginname" />			<field name="password"/>            <field name="sessionkey"/>            <field name="state"/>            <field name="extra"/>		</insert>		<list>			<field name="id" />			<field name="owner" />			<field name="loginname" />            <field name="state"/> 		</list>		<query>			<field name="id" />			<field name="owner" />			<field name="loginname" />			<field name="password"/>            <field name="sessionkey"/>            <field name="state"/>            <field name="creationdate"/>            <field name="modificationdate"/>            <field name="extra"/>		</query> 	</views>	<data>		<field name="id" type="INTEGER" required="true" key="true" />		<field name="owner" type="STRING" size="12" required="true" default="unknown" />		<field name="loginname" type="STRING" size="64" required="true" />		<field name="password" type="STRING" size="64" required="true" />        <field name="sessionkey" type="STRING" size="64" required="false" />        <field name="state" type="INTEGER" required="true" />        <field name="creationdate" type="TIMESTAMP"/>        <field name="modificationdate" type="TIMESTAMP"/>		<field name="extra" type="XML" />	</data></table>
<?xml version="1.0" encoding="iso-8859-1"?>

<table>

    <views>

		<insert>
			<field name="request" />
			<field name="response" />
        </insert>

		<list>
			<field name="id" />
			<field name="request" />
			<field name="response" />
          </list>

		<query>
			<field name="id" />
			<field name="request" />
			<field name="response" />
   		</query>
 	</views>

	<data>
		<field name="id" type="INTEGER" required="true" key="true" />
		<field name="request" type="XML" required="false"/>
		<field name="response" type="XML" required="false"/>
        <field name="creationdate" type="TIMESTAMP"/>
		<field name="modificationdate" type="TIMESTAMP"/>
    </data>
</table>
<testset id="connect" clients="1">

	<test id="login-select-app-enter-join">
		<send id="client1">
			<login-req name="mobgame-user" password="user" portalname="games" protocolVersion="4.0"/>
			<select-app-req appname="mobgame" rolename="user"/>
			<enter-space-req spacename="wp-space"/>
			<join-amulet-req id="wp-amulet" protocolversion="2.0">
				<options type="gamephone" name="+31613362231"/>
			</join-amulet-req>
		</send>

		<expect id="client1">
			<login-rsp/>
			<select-app-rsp/>
			<enter-space-rsp/>
			<join-amulet-rsp/>
		</expect>
	</test>


	<test id="errored req">
		<send id="client1">
			<amulet id="mobgame">
				<bla-req a="b" etc="c"/>
			</amulet>
		</send>

		<expect id="client1">
			<amulet id="mobgame">
				<bla-nrsp error=""/>
			</amulet>
		</expect>
	</test>

	<test id="leave-exit-logout">
		<send id="client1">
			<leave-amulet-req id="mobgame"/>
			<exit-space-req/>
			<logout-req/>
		</send>

		<expect id="client1">
			<leave-amulet-rsp id="mobgame"/>
			<exit-space-rsp/>
			<logout-rsp/>
		</expect>
	</test>


</testset>


				<tour-get-rsp>
					<tour id="${tourid1}">
						<id>${tourid1}</id>
						<owner>unknown</owner>
						<name>MyFirstTour</name>
						<description>Echt een hele mooie tour</description>
						<invitation>Komt allen</invitation>
						<intro/>
						<outro/>
						<startdate>5000111222</startdate>
						<enddate>5000111223</enddate>
						<creationdate>1169551751073</creationdate>
						<modificationdate>1169551751073</modificationdate>
						<extra>
							<invitees/>
						</extra>
					</tour>
				</tour-get-rsp>
			</utopia-rsp>
		</expect>
	</test>

